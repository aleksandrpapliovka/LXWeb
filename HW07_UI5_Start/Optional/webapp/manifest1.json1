{
  "_version": "1.12.0",
  "sap.app": {
	"id": "hw07.t2.webapp",
	"type": "application",
	"i18n": "i18n/i18n.properties",
	"title": "{{appTitle}}",
	"description": "{{appDescription}}",
	"applicationVersion": {
	  "version": "1.0.0"
	}
	
